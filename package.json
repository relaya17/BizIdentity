{
  "name": "user-system-monorepo",
  "version": "1.0.0",
  "description": "A monorepo for user management system",
  "main": "index.js",
  "private": true,
  "packageManager": "pnpm@10.11.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "pnpm exec run-p dev:client dev:server",
    "dev:client": "pnpm --filter client dev",
    "dev:server": "pnpm --filter server dev",
    "build": "pnpm -r run build",
    "build:client": "pnpm --filter client build",
    "build:server": "pnpm --filter server build",
    "lint": "eslint . --ext .ts,.tsx,.js --ignore-path .gitignore",
    "clean": "pnpm -r exec rimraf dist .tsbuildinfo",
    "clean:all": "rimraf node_modules dist build .tsbuildinfo && pnpm clean",
    "start:server": "pnpm --filter server start"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^5.16.4",
    "@mui/material": "^5.16.4",
    "@react-oauth/google": "^0.12.2",
    "@reduxjs/toolkit": "^2.8.2",
    "@types/qs": "^6.14.0",
    "axios": "^1.9.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.19.2",
    "express-validator": "^7.2.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.5.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.14",
    "qs": "^6.14.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-leaflet": "^4.2.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.25.1",
    "react-toastify": "^11.0.5",
    "redux": "^5.0.1",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.4",
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.21",
    "@types/express-serve-static-core": "^4.17.4",
    "@types/express-validator": "^3.0.2",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/leaflet": "^1.9.18",
    "@types/mongoose": "^5.11.97",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.14.10",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/react-helmet": "^6.1.11",
    "@types/react-redux": "^7.1.34",
    "@types/react-toastify": "^4.1.0",
    "@types/uuid": "^10.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jsonwebtoken": "^9.0.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.7",
    "sass": "^1.77.8",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.3",
    "vite": "^5.3.3"
  }
}